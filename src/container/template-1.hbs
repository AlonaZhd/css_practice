	<div class='page'>
		<div class="page_block">
			<header class='header'>
				{{> logo}}
				<div class='header_nav'>
					{{>nav list=header}}
				</div>
			</header>
			{{> line}}
		</div>
		<div class="page_block">
			{{> hero data=hero}}
		</div>
		<div class="page_block">
			<div class='plans row'>
				{{#each plans}}
					<div class="col plans_card">
						{{> card data=this}}
					</div>
				{{/each}}
			</div>
		</div>
		<div class="page_block">
			{{> line}}
			<footer class="footer row">
				<div class="col-3">
					{{> logo}}
					<span class="footer_copyright">Copyright 2023</span>
				</div>
				{{#each footer}}
					<div class="col-3">
						<span class="footer_title">
							{{title}}
						</span>
						<span class="footer_links">
							{{>nav list=links isVertical=true}}
						</span>
					</div>
				{{/each}}
				
			</footer>
		</div>
	</div>